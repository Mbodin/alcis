(* Product and sum types. *)

??a * ?b ::= ??a , b.

??a + ?b ::=
	| a /
	| / b
	.

?a. ?b. (a * b) ((a _ a : bool.bool) = (b _ b : bool.bool)) (a * b) : bool.bool.
?a. ?b. (a + b) ((a _ a : bool.bool) = (b _ b : bool.bool)) (a + b) : bool.bool.

?a. ?b. swap (a * b) : b * a.

?a. ?b. swap (a + b) : b + a.

?a. ?b. fst (a * b) : a.
?a. ?b. snd (a * b) : b.

basic.|>

?a. absurdl (void * a) : void.
?a. absurdr (a * void) : void.
?a. absurdl (void + a) : a.
?a. absurdr (a + void) : a.

